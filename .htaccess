### PROTECT FILES ###
<FilesMatch "\.(htaccess|htpasswd|ini|fla|psd|log|sh)$">
Order Allow,Deny
Deny from all
</FilesMatch>

RewriteEngine on
RewriteRule zatrudnij_programiste index.html
RewriteRule kim_jestem kimjestem.html
RewriteRule oferta oferta.html
RewriteRule cv cv.html
RewriteRule kontakt kontakt.html
ErrorDocument 404 /index.html
